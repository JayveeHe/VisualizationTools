<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="jayvee.visualization_test"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="15" />

    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>

    <application
        android:allowBackup="true"
        android:icon="@drawable/xiaohuang"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="surfaceview_test.SurfaceViewMain"
            android:label="复杂网络静态绘图" >
        </activity>
        <activity
            android:name="surfaceview_test.MainActivity"
            android:label="可视化研究" >
        </activity>
        <activity
            android:name="ui.NaviActivity"
            android:label="数据挖掘可视化" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="ui.ListActivity"
            android:label="文件列表" >
        </activity>
        <activity android:name="ui.Viewpager_main"></activity>
    </application>

</manifest>